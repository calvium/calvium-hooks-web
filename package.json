    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@types/jest": "^24.0.22",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "babel-loader": "^8.0.6",
    "eslint-plugin-react-hooks": "^2.2.0",
    "husky": "^3.0.9",
    "prettier": "1.19.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "tsdx": "^0.11.0",
    "tslib": "^1.10.0",
    "typescript": "^3.7.2"
