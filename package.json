{
  "name": "calvium-hooks-web",
  "version": "0.1.0",
  "license": "MIT",
  "author": "Ben Clayton",
  "main": "dist/index.js",
  "module": "dist/calvium-hooks-web.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --env=jsdom --passWithNoTests",
    "lint": "tsdx lint",
    "publish-local": "npm run build && npm unpublish --registry http://localhost:4873/ --force && npm publish --registry http://localhost:4873/",
    "check-all-changed": "npm run check-prettier --silent && npm run check-eslint-changed --silent && npm run check-tsc --silent && npm run test",
    "check-eslint-changed": "./scripts/eslint-only-changed.sh",
    "check-eslint": "eslint \"src/**/**.?(ts|tsx)\" --fix --max-warnings=0",
    "check-all": "npm run check-prettier --silent && npm run check-eslint --silent && npm run check-tsc --silent && npm run test",
    "check-tsc": "tsc --noEmit",
    "check-prettier": "prettier --config .prettierrc.js --list-different --write 'src/**/**.?(ts|tsx|js|jsx|md|css)'"
  },
  "peerDependencies": {
    "react": ">=16",
    "styled-components": ">= 4.0.0",
    "react-router-dom": ">= 5.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@calvium/eslint-config": "^1.0.2",
    "@react-native-community/eslint-config": "0.0.5",
    "@types/jest": "^24.0.22",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "5.1.2",
    "@types/styled-components": "4.1.22",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "babel-loader": "^8.0.6",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.3",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "eslint-plugin-react-native": "^3.8.1",
    "husky": "^3.0.9",
    "react": "^16.11.0",
    "react-docgen-typescript-loader": "^3.3.0",
    "react-dom": "^16.11.0",
    "react-router-dom": "5.1.2",
    "styled-components": "4.4.1",
    "tsdx": "^0.11.0",
    "tslib": "^1.10.0",
    "typescript": "^3.7.2",
    "@calvium/prettier-config": "1.0.1",
    "prettier": "^1.18.2"
  },
  "dependencies": {}
}
